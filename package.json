{
  "name": "hapi-waypointer",
  "version": "0.2.4",
  "description": "A HAPI plug-in that provides modern auto documentation of APIs using Waypointer themes",
  "keywords": [
    "waypointer",
    "modern",
    "documentation",
    "swagger",
    "har"
  ],
  "main": "index.js",
  "scripts": {
    "dev-debug": "node --inspect-brk=9229 ./lib/index.js",
    "dev": "nodemon",
    "start": "node ./lib/index.js",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "pretty": "prettier --write \"./**/*.{js,scss,json}\"",
    "test": "jest",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "author": "Glenn Jones <glennjonesnet@gmail.com> (http://glennjones.net/)",
  "license": "MIT",
  "dependencies": {
    "@glennjones/nunjucks-tools": "0.0.2",
    "@hapi/hoek": "^9.1.0",
    "@hapi/joi": "17.1.1",
    "async": "^3.2.0",
    "chalk": "^1.1.1",
    "commander": "^2.9.0",
    "debug": "^2.2.0",
    "event-stream": "3.3.4",
    "form-data": "3.0.0",
    "fs-readfile-promise": "^2.0.1",
    "fs-writefile-promise": "^1.0.3",
    "hapi-swagger": "^13.1.0",
    "har-validator": "^5.1.5",
    "highlight.js": "^9.18.3",
    "js-beautify": "^1.13.0",
    "json-schema-ref-parser": "^7.1.4",
    "marked": "^1.2.3",
    "nunjucks": "^3.2.2",
    "pinkie-promise": "^2.0.0",
    "slugify": "^1.4.6",
    "stringify-object": "^3.3.0",
    "swagger-exemplar": "^0.4.2",
    "swagger-har": "^2.0.3"
  },
  "devDependencies": {
    "@hapi/code": "^8.0.2",
    "@hapi/good": "^9.0.1",
    "@hapi/good-console": "^9.0.1",
    "@hapi/hapi": "^19.2.0",
    "@hapi/inert": "^6.0.3",
    "@hapi/lab": "^22.0.5",
    "@hapi/vision": "^6.0.1",
    "blipp": "^4.0.1",
    "coveralls": "^3.1.0",
    "eslint": "^6.8.0",
    "js2xmlparser": "^4.0.1",
    "waypointer-plain": "^0.3.1"
  }
}
